      font-size: 1.2rem;
      line-height: 1.6;
      color: #5b5b5b;
      margin-bottom: 26px;
    }
    .bdny-hero__btn {
      display: inline-block;
      background: #000;
      color: #fff;
      padding: 12px 28px;
      border-radius: 999px;
      font-weight: 600;
      text-decoration: none;
      transition: opacity .15s ease;
    }
    .bdny-hero__btn:hover {
      opacity: .85;
      color: #fff;
    }

    .bdny-hero__media {
      width: 100%;
      max-height:600px;
      border-radius: 22px;
      overflow: hidden;
      background: #f5f5f5;
      min-height: 260px;
    }
    .bdny-hero__media img {
      width: 100%;
      max-height: 500px;
      object-fit: cover;
      display: block;
    }
    .bdny-hero__placeholder {
      padding: 60px 20px;
      text-align: center;
      color: #888;
      font-size: 14px;
    }

    /* mobile */
    @media (max-width: 992px) {
      .bdny-hero__inner {
        grid-template-columns: 1fr;
      }
      .bdny-hero__title {
        font-size: clamp(2rem, 8vw, 2.5rem);
