 <!-- klaviyo search tracking -->
<script defer>
  document.addEventListener('DOMContentLoaded', function () {
    function setupSearchTracking(containerSelector) {
      const container = document.querySelector(containerSelector);
      if (!container) {
        return;
      }

      const input = container.querySelector('input[type="text"], input[type="search"]');
      const button = container.querySelector('button, a.search-button, .search-button');

      if (!input) console.warn(`Input not found in ${containerSelector}`);
      if (!button) console.warn(`Button not found in ${containerSelector}`);

      function handleSearch() {
        const searchTerm = input ? input.value.trim() : '';
        if (!searchTerm) {
          return;
        }
        let attempts = 0;
        const maxAttempts = 10;

        const interval = setInterval(() => {
          attempts++;
          if (typeof _learnq !== 'undefined') {
            _learnq.push(['track', 'Submitted Search', { search_term: searchTerm }]);
            _learnq.push(['track', 'Search Query', { search_term: searchTerm }]);
            clearInterval(interval);

            // Give the browser time to send the event
            setTimeout(() => {
              window.location.href = `/search?q=${encodeURIComponent(searchTerm)}`;
            }, 300);
          } else if (attempts >= maxAttempts) {
            clearInterval(interval);
            window.location.href = `/search?q=${encodeURIComponent(searchTerm)}`;
          }
        }, 2000);
      }

      if (input) {
        input.addEventListener('keypress', function (e) {
          if (e.key === 'Enter') {
            e.preventDefault();
            handleSearch();
          }
        });
      }

      if (button) {
        button.addEventListener('click', function (e) {
          e.preventDefault();
          handleSearch();
        });
      }
    }

    setupSearchTracking('.DesktopSearch');
    setupSearchTracking('.PhoneSearch');
  });
</script>
<!-- end klaviyo search tracking --> 

